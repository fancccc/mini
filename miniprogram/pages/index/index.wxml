				<image class="weui-grid__icon" src="../../images/icon_tabbar.png" alt></image>
			</view>
			<view class="weui-grid__label">计时器</view>
		</navigator>
		<navigator class="weui-grid" aria-role="button" url="../bill/bill">
			<view class="weui-grid__icon">
				<image class="weui-grid__icon" src="../../images/icon_tabbar.png" alt></image>
			</view>
			<view class="weui-grid__label">账单</view>
		</navigator>
		<navigator class="weui-grid" aria-role="button" url="javascript:">
			<view class="weui-grid__icon">
				<image class="weui-grid__icon" src="../../images/icon_tabbar.png" alt></image>
			</view>
			<view class="weui-grid__label">Grid</view>
		</navigator>
		<navigator class="weui-grid" aria-role="button" url="javascript:">
			<view class="weui-grid__icon">
				<image class="weui-grid__icon" src="../../images/icon_tabbar.png" alt></image>
			</view>
			<view class="weui-grid__label">Grid</view>
		</navigator>
		<navigator class="weui-grid" aria-role="button" url="javascript:">
			<view class="weui-grid__icon">
				<image class="weui-grid__icon" src="../../images/icon_tabbar.png" alt></image>
			</view>
			<view class="weui-grid__label">Grid</view>
		</navigator>
  </view>
